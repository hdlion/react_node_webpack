{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "start": "npm run local",
    "local": "webpack-dashboard run-p local:*",
    "local:variable": "APP_PUBLIC_PATH=http://localhost:8080/build/client/ APP_SSR=false NODE_ENV=development",
    "local:client": "./node_modules/.bin/webpack -w",
    "local:server": "run-s local:server:*",
    "local:server:build": "babel ./application/server -d ./build/server --presets env,stage-2",
    "local:server:run": "node ./build/server/server.js",
    "production": "npm install && run-p production:*",
    "production:variable": "APP_PUBLIC_PATH=http://localhost:8080/build/client/ APP_SSR=true NODE_ENV=production",
    "production:client": "./node_modules/.bin/webpack",
    "production:server": "run-s production:server:*",
    "production:server:build": "babel ./application/server -d ./build/server --presets env,stage-2",
    "production:server:run": "pm2 start pm2.json --only 3idiots",
    "clustorstop": "pm2 delete pm2.json",
    "test": "mocha --compilers babel-core/register ./test/*test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "pm2": "^2.9.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^1.0.2"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
